[application]

config/name="ControlSD"
config/description="Command & Collab is an action puzzle platformer about a warm relationship between a father and son; 
The Caretaker's farm has suffered a cyber attack when he was secretly away with his adopted cub Charlie. 
He and Charlie must take the farm back before the virus destroys the farm.
"
run/main_scene="res://src/menu/Menu.tscn"
boot_splash/bg_color=Color( 0, 0, 0, 1 )
config/icon="res://icon.png"

[autoload]

Global="*res://src/AutoLoad/Global.gd"
SceneChanger="*res://src/AutoLoad/SceneChanger.tscn"
Shake="*res://src/AutoLoad/Shake.gd"

[display]

window/size/test_width=1024
window/size/test_height=600
window/stretch/mode="2d"
window/stretch/aspect="keep"

[editor_plugins]

enabled=PoolStringArray(  )

[gdnative]

singletons=[ "res://addons/godot-git-plugin/git_api.gdnlib" ]

[global]

background=false

